{
  "name": "LightbaseTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "androidStaging": "react-native run-android --variant=stagingRelease",
    "androidRelease": "react-native run-android --variant=release",
    "ios": "ENVFILE=.env.staging react-native run-ios",
    "iosRelease": "ENVFILE=.env.production react-native run-ios --configuration=Release",
    "iosStaging": "ENVFILE=.env.staging react-native run-ios --configuration=Staging",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "pretty": "prettier --check 'src/**/*.{ts,tsx}' --write",
    "tsCheck": "tsc -p ./",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-community/async-storage": "latest",
    "@react-native-community/masked-view": "latest",
    "@react-native-community/netinfo": "latest",
    "@react-navigation/bottom-tabs": "latest",
    "@react-navigation/native": "latest",
    "axios": "latest",
    "bugsnag-react-native": "latest",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-bootsplash": "latest",
    "react-native-code-push": "latest",
    "react-native-config": "latest",
    "react-native-device-info": "latest",
    "react-native-gesture-handler": "latest",
    "react-native-keyboard-aware-scroll-view": "latest",
    "react-native-reanimated": "latest",
    "react-native-redash": "latest",
    "react-native-safe-area-context": "latest",
    "react-native-screens": "latest",
    "react-native-svg": "latest",
    "react-native-unimodules": "latest",
    "react-query": "latest",
    "url-parse": "latest"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/runtime": "latest",
    "@lightbase/eslint-config-lightbase": "latest",
    "@testing-library/react-native": "latest",
    "@types/axios": "latest",
    "@types/jest": "latest",
    "@types/react": "latest",
    "@types/react-native": "latest",
    "@types/react-test-renderer": "latest",
    "babel-jest": "latest",
    "babel-plugin-transform-remove-console": "latest",
    "eslint": "latest",
    "jest": "latest",
    "metro-react-native-babel-preset": "latest",
    "patch-package": "latest",
    "postinstall-postinstall": "latest",
    "prettier": "latest",
    "react-native-flipper": "^0.47.0",
    "react-native-testing-library": "latest",
    "react-query-native-devtools": "latest",
    "react-test-renderer": "16.11.0",
    "reactotron-react-native": "latest",
    "stop-runaway-react-effects": "latest",
    "typescript": "latest"
  }
}
