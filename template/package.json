{
  "name": "LightbaseTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "androidStaging": "react-native run-android --variant=stagingRelease",
    "androidRelease": "react-native run-android --variant=release",
    "ios": "ENVFILE=.env.staging react-native run-ios",
    "iosRelease": "ENVFILE=.env.production react-native run-ios --configuration=Release",
    "iosStaging": "ENVFILE=.env.staging react-native run-ios --configuration=Staging",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "pretty": "prettier --check 'src/**/*.{ts,tsx}' --write",
    "tsCheck": "tsc -p ./",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bugsnag-react-native": "^2.23.10",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-bootsplash": "^2.2.5",
    "react-native-code-push": "^6.3.0",
    "react-native-config": "1.3.1",
    "react-native-device-info": "5.6.2",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-reanimated": "1.9.0",
    "react-native-redash": "14.2.3",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.9.0",
    "react-native-svg": "^12.1.0",
    "react-native-unimodules": "^0.10.1",
    "react-query": "^2.5.4",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/runtime": "^7.10.5",
    "@lightbase/eslint-config-lightbase": "^2.13.1",
    "@testing-library/react-native": "^5.0.3",
    "@types/jest": "^26.0.5",
    "@types/react": "^16.9.43",
    "@types/react-native": "^0.63.1",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "26.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.5.0",
    "jest": "26.1.0",
    "metro-react-native-babel-preset": "0.60.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.0.5",
    "react-native-flipper": "^0.47.0",
    "react-native-testing-library": "^2.1.1",
    "react-query-native-devtools": "^0.1.5",
    "react-test-renderer": "16.11.0",
    "reactotron-react-native": "^5.0.0",
    "stop-runaway-react-effects": "^2.0.0",
    "typescript": "^3.9.7"
  }
}
